package Hw2;

import java.awt.Color;
import java.awt.Graphics;

public class Square {
	int x, y, w,h;
	
	public Square(int x, int y, int w, int h) {
		this.x=x;
		this.y=y;
		this.w=w;
		this.h=h;
	}

	public void draw(Graphics g2) {
		// TODO Auto-generated method stub
		g2.setColor(Color.BLUE);
		g2.drawRect(x, y, w, h);
		
		
	}
}
